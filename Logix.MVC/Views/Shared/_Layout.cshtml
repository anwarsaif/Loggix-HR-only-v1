@inject IMainServiceManager serviceManager
@inject IAuthService authService
@inject ICurrentData session
@inject IMvcSession mvcSession
@inject ILocalizationService localization
@inject IDDListHelper listHelper
@inject IAccServiceManager accServiceManager
@inject ISysConfigurationHelper configurationHelper


@{
    // int lang = session.GetData<int>("language");
    // var currUser = session.GetData<SysUser>("user");
    int lang = mvcSession.GetData<int>("language");
    var currUser = mvcSession.GetData<SysUser>("user");
    int sysId = 1;
    if (ViewData.ContainsKey("SysId"))
    {
        if (ViewData["SysId"] is int)
        {
            sysId = (int)ViewData["SysId"];

        }
        else
        {
            // handle the case where the SysId is not an integer
        }
    }


    var allUsers = new List<SysUserVw>();
    var getUsers = await authService.GetUsersByEmpId(session.EmpId, session.UserId);
    if (getUsers.Succeeded)
    {
        allUsers = getUsers.Data.ToList();
    }
    //---------------
    var allFacilityId = new AccFacilityDto();
    var getFacility = await accServiceManager.AccFacilityService.GetById(session.FacilityId);
    if (getFacility.Succeeded)
    {
        allFacilityId = getFacility.Data;
    }
    var allNotifications = new List<SysNotificationDto>();
    var getNotifs = await serviceManager.SysNotificationService.GetAll();
    if (getNotifs.Succeeded)
    {
        allNotifications = getNotifs.Data.Where(n => n.UserId == currUser.Id && n.IsRead == false).ToList();

    }
    
    var allTasks = new List<TsTasksVwDto>();
    var getTasks = await serviceManager.SysSystemService.GetTasksByUser(session.UserId);
    if (getTasks.Succeeded)
    {
        allTasks = getTasks.Data.ToList();

    }


    var allFinYear = new List<AccFinancialYear>();
    var getFinYears = await serviceManager.SysSystemService.GetFinancialYears();
    if (getFinYears.Succeeded)
    {
        allFinYear = getFinYears.Data.Where(n => n.FacilityId == currUser.FacilityId && n.IsDeleted == false).ToList();

    }
     var ddFinYears = listHelper.GetFromList<long>(allFinYear.Select(s => new DDListItem<long> { Name = s.FinYearGregorian.ToString(), Value = s.FinYear }).Reverse(),hasDefault:false, selectedValue:session.FinYear);


    var allSys = new List<SysSystemDto>();
    var currSys = new SysSystemDto();
    var getSys = await serviceManager.SysSystemService.GetById(sysId);
    if (getSys.Succeeded)
    {
        currSys = getSys.Data;
    }
    //var mainList = new List<MainListDto>();
    //var getList = await serviceManager.SysSystemService.GetMainList(1, sysId, 13);
    //if (getList.Succeeded)
    //{
    //    mainList = getList.Data.ToList();
    //}



    var systems = await serviceManager.SysSystemService.GetAll();
            if (systems.Succeeded)
            {
              allSys=  systems.Data.Where(s=>s.Isdel == false && s.ShowInHome == true ).OrderBy(s=>s.SysSort).ToList();            
            }
    string Facilityurl = await configurationHelper.GetValue(266, session.FacilityId);

}


